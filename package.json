{
  "name": "typescript-collections",
  "version": "1.1.0",
  "description": "A complete, fully tested data structure library written in TypeScript.",
  "main": "./dist/umd.js",
  "typings": "./dist/lib/index.d.ts",
  "scripts": {
    "install_tools": "npm install && typings install",
    "clean": "rimraf ./dist",
    "lint": "tslint -c ./tslint.json ./src/**/*.ts",
    "tsc": "tsc",
    "umd": "node browserify-umd.js",
    "minify": "node minify-umd.js",
    "build": "npm run lint && npm run tsc && npm run umd && npm run minify",
    "test": "mocha ./dist/test/*Test.js",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- ./dist/test/*.js",
    "all": "npm run clean && npm run build && npm run test && npm run cover"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/basarat/typescript-collections.git"
  },
  "keywords": [
    "typescript",
    "generics",
    "data",
    "structures",
    "collections",
    "linked",
    "list",
    "dictionary",
    "multi",
    "dictionary",
    "binary",
    "search",
    "tree",
    "stack",
    "queue",
    "set",
    "bag",
    "binary",
    "heap",
    "priority",
    "queue",
    "array"
  ],
  "author": "Basarat Ali Syed <bas@basarat.com> (http://basarat.com)",
  "contributors": [
    "Tomasz Ciborski <episage>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basarat/typescript-collections/issues"
  },
  "homepage": "https://github.com/basarat/typescript-collections",
  "dependencies": {},
  "devDependencies": {
    "assert": "^1.3.0",
    "browserify": "^13.0.0",
    "browserify-umdify": "^1.0.3",
    "chai": "^2.3.0",
    "glob": "^7.0.0",
    "istanbul": "^0.4.2",
    "jasmine": "^2.4.1",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.4",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.2.10",
    "tslint": "^3.3.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "uglify-js": "^2.6.2",
    "util": "^0.10.3"
  }
}
